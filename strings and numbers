var a = Number(a);
 var b = Number(b);
 var sum = a+b;
 return String(sum); // как перевести строку в число и сложить числа, а потом обратно вернуть в виде строки


function sumStr(a,b) {
  let numOne = Number(a)
  let numTwo = Number(b)
  let sum = numOne + numTwo
  return sum.toString()
}


function numberToString(num) {
  return String(num); // Return a string of the number here!
}




numberToString = String;
