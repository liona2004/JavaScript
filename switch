// switch сравнивает выражение со случаями, перечисленными внутри switch, а затем выполняет соответсвующие инструкции
switch (expression) {     // expression - выражение, знначение которого сравнивается со всеми случаями
  case value1:   
    // здесь выполняются инструкции, если результат выражения равен value1 statement1
    break;
  case value2: 
    // инструкции, соответствующие value2 statement2
    break;
  default:   
    //здесь находятся инструкции, которые выполняются при отсутствии соответствующего значения statements_def
}

// case valueN - это случай, который проверяется на соответствие выражению (exxpression)
// statementN - инструкции, которые выполняются, если выражение соответствует случаю
//  statements_def - инструкции, которые выполняются, если выражение не соответствует ни одному случаю

function caseInSwitch(val) {
  let answer = "";
  // Only change code below this line
  switch(val) {
    case 1:
      answer = "alpha";
      break;
    case 2:
      answer = "beta";
      break;
    case 3:
      answer = "gamma";
      break;
    case 4:
      answer = "delta";
      break;
   }


  // Only change code above this line
  return answer;
}

caseInSwitch(1);
